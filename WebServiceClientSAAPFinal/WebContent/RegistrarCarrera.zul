<?page title="REGISTRO DE CARRERA" contentType="text/html;charset=UTF-8"?>
<zk>
<window title="REGISTRO DE CARRERA" border="normal" apply="com.servicios.controladoes.RegistrarCarreraControlador">
<grid width="80%">
<rows >
	<row>
		<label value="Tarifa:" />
		<combobox id="cbbTarifa"  autodrop="true" width="100%">
	
				<comboitem label="Diurna" value="1" />
				<comboitem label="Nocturna" value="2" />
		</combobox >
	</row>
	<row>
		<label value="Pasajero:" />
		<textbox id="textbox_pasajero" constraint="no empty" />
	</row>
	<row>
		<label value="Origen:" />
		<textbox id="textbox_origen" constraint="no empty" />
	</row>
	<row>
		<label value="Destino:" />
		<textbox id="textbox_destino" constraint="no empty" />
	</row>
	<row>
		<label value="Tiempo:" />
		<textbox id="textbox_tiempo" constraint="no empty" />
	</row>
	<row>
		<label value="Kilometros recorridos:" />
		<doublebox id="textbox_velocidad" constraint="no empty" />
	</row>
	<row>
		<label value="Precio:" />
		<doublebox id="textbox_precio" constraint="no empty" />
	</row>
	<row>
		<label value="Fecha:" />
		<datebox id="datebox_Inicio" format="mm/dd/yy"/>
	</row>
	<row>
		<label value="Usuario:" />
		<textbox id="textbox_usuario" constraint="no empty" />
	</row>
	<row>
		<label value="ContraseÃ±a:" />
		<textbox id="textbox_contrasena" constraint="no empty" />
	</row>
	<row>
		<label value="latitud origen:" />
		<doublebox id="textbox_latitud_origen" constraint="no empty" />
	</row>
	<row>
		<label value="longitud origen:" />
		<doublebox id="textbox_longitud_origen" constraint="no empty" />
	</row>
	<row>
		<label value="latitud destino:" />
		<doublebox id="textbox_latitud_destino" constraint="no empty" />
	</row>
	<row>
		<label value="longitud destino:" />
		<doublebox id="textbox_longitud_destino" constraint="no empty" />
	</row>
	<row>
		<button label="GUARDAR" onClick="" id="btnGuardar"/>
		<button label="CANCELAR" onClick="" id="btnCancelar"/>
	</row>
</rows>
</grid>
</window>
</zk>